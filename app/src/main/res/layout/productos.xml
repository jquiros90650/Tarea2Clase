<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BCF6E7BE">

    <TextView
        android:id="@+id/et_productos2"
        android:layout_width="409dp"
        android:layout_height="38dp"
        android:text="LO MAS NOVEDOSO"
        android:textAlignment="center"
        android:textColor="#2962FF"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="400dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="416dp"
        android:layout_height="292dp"
        android:layout_marginTop="108dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/amazon"
        tools:layout_editor_absoluteX="-2dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="409dp"
        android:layout_height="290dp"
        app:srcCompat="@drawable/amazon2"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="438dp" />

    <TextView
        android:id="@+id/et_productos"
        android:layout_width="409dp"
        android:layout_height="44dp"
        android:text="COMPRAS ULTIMOS 6 MESES"
        android:textAlignment="center"
        android:textColor="#DD2C00"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="66dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="63dp"
        android:layout_height="53dp"
        app:srcCompat="@drawable/ic_add_shopping_cart"
        tools:layout_editor_absoluteX="289dp"
        tools:layout_editor_absoluteY="4dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="58dp"
        android:layout_height="53dp"
        app:srcCompat="@drawable/ic_logout"
        tools:layout_editor_absoluteX="352dp"
        tools:layout_editor_absoluteY="4dp" />
</androidx.constraintlayout.widget.ConstraintLayout>